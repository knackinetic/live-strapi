{
  "routes": {
    "GET /settings/general": {
      "method": "GET",
      "path": "/settings/general",
      "handler": "SettingsManager.getGeneralSettings",
      "policies": []
    },
    "PUT /settings": {
      "action": "updateSettings",
      "method": "PUT",
      "path": "/settings",
      "handler": "SettingsManager.updateSettings",
      "policies": []
    },
    "GET /:file": {
      "action": "updateSettings",
      "method": "GET",
      "path": "/:file",
      "handler": "SettingsManager.file",
      "policies": []
    }
  }
}
